@model List<Airways.ViewModels.BookingVM>
@{
    int profileId = 0;
    if (ViewData["ProfileId"] != null)
    {
        profileId = (int)ViewData["ProfileId"];
    }
}

@if (Model == null || !Model.Any())
{
    <partial name="_NoBookings" />
}
else
{
    <!-- Your existing code for displaying bookings -->
    <div class="table-responsive">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Booking ID</th>
                    <th>Date</th>
                    <th>From</th>
                    <th>To</th>
                    <th>Class</th>
                    <th>Seat</th>
                    <th>Status</th>
                    <th>Price</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var booking in Model)
                {
                    <tr class="@(booking.Status == "Cancelled" ? "table-danger" : (booking.Status == "Done" ? "table-secondary" : ""))">
                        <!-- Your booking display fields -->
                        <!-- Add your booking display code here -->
                    </tr>
                }
            </tbody>
        </table>
    </div>
}